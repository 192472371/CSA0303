#include <stdio.h>

int main() {
    int list1[50], list2[50], merged[100];
    int n1, n2, i, j;

    printf("Enter number of elements in first list: ");
    scanf("%d", &n1);
    printf("Enter elements of first list: ");
    for (i = 0; i < n1; i++)
        scanf("%d", &list1[i]);

    printf("Enter number of elements in second list: ");
    scanf("%d", &n2);
    printf("Enter elements of second list: ");
    for (i = 0; i < n2; i++)
        scanf("%d", &list2[i]);

    for (i = 0; i < n1; i++)
        merged[i] = list1[i];
    for (j = 0; j < n2; j++)
        merged[n1 + j] = list2[j];

    printf("Merged List: ");
    for (i = 0; i < n1 + n2; i++)
        printf("%d ", merged[i]);

    return 0;
}
