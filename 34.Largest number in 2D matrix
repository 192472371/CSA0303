#include <stdio.h>

int main() {
    int rows, cols, i, j, max;
    int arr[50][50];

    printf("Enter rows and columns: ");
    scanf("%d %d", &rows, &cols);

    printf("Enter elements:\n");
    for (i = 0; i < rows; i++)
        for (j = 0; j < cols; j++)
            scanf("%d", &arr[i][j]);

    max = arr[0][0];
    for (i = 0; i < rows; i++)
        for (j = 0; j < cols; j++)
            if (arr[i][j] > max)
                max = arr[i][j];

    printf("Largest element: %d\n", max);
    return 0;
}
